{
    "name": "illusionist/flysystem-aliyun-oss",
    "description": "Flysystem adapter for the Aliyun OSS SDK",
    "keywords": [
        "filesystem", "aliyun oss", "laravel", "lumen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "kayson",
            "email": "kayson@huida.cn"
        }
    ],
    "require": {
        "php":">=5.5.9",
        "aliyuncs/oss-sdk-php": "^2.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Illusionist\\Flysystem\\Aliyun\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illusionist\\Flysystem\\Aliyun\\OssServiceProvider"
            ]
        }
    }
}
